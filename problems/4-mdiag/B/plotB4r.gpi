set term svg font ",16" background rgb "white" size 900,500
set title "Rotations jacobi cyclic vs full value-by-value decomposition"
set key rmargin
set xlabel "size n (symmetric matrix A nxn)"
set ylabel "Average rotations needed"
set out "PlotB4r.svg"
set logscale y

plot "out.b4rot.txt" u 1:2 with lines title "value-by-value"\
	, "out.b4rot.txt" u 1:3 with lines title "cyclic"
